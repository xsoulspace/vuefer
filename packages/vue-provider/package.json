{
  "name": "@xsoulspace/vue-provider",
  "description": "Dependecy injection provider heavily inspired by Flutter Provider",
  "version": "1.0.2",
  "private": false,
  "author": {
    "name": "Anton Malofeev",
    "email": "anton@xsoulspace.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xsoulspace/vuefer.git"
  },
  "bugs": {
    "url": "https://github.com/xsoulspace/vuefer/issues"
  },
  "homepage": "https://github.com/xsoulspace/vuefer/#readme",
  "main": "dist/vue-provider.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-provider.es.js",
      "require": "./dist/vue-provider.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "serve": "vite preview",
    "upgrade": "yarn yarn-upgrade-all add vue@next",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "patch": "yarn version --patch && git push && git push origin --tags",
    "minor": "yarn version --minor && git push && git push origin --tags"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "vue": "^3.2.27"
  },
  "peerDependencies": {
    "vue": "^3.2.27"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/compiler-sfc": "^3.2.27",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "sass": "^1.48.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4",
    "vite": "^2.7.12"
  }
}
